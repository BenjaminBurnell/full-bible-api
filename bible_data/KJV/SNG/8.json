{
  "version": "KJV",
  "book": "SNG",
  "chapter": 8,
  "verses": [
    {
      "verse": "1",
      "text": "O that thou wert as my brother,"
    },
    {
      "verse": "2",
      "text": "I would lead thee,"
    },
    {
      "verse": "3",
      "text": "His left hand should be under my head,"
    },
    {
      "verse": "4",
      "text": "I charge you, O daughters of Jerusalem,"
    },
    {
      "verse": "5",
      "text": "Who is this that cometh up from the wilderness,"
    },
    {
      "verse": "6",
      "text": "Set me as a seal upon thine heart,"
    },
    {
      "verse": "7",
      "text": "Many waters cannot quench love,"
    },
    {
      "verse": "8",
      "text": "We have a little sister,"
    },
    {
      "verse": "9",
      "text": "If she be a wall,"
    },
    {
      "verse": "10",
      "text": "I am a wall,"
    },
    {
      "verse": "11",
      "text": "Solomon had a vineyard at Baalhamon;"
    },
    {
      "verse": "12",
      "text": "My vineyard, which is mine, is before me:"
    },
    {
      "verse": "13",
      "text": "Thou that dwellest in the gardens,"
    },
    {
      "verse": "14",
      "text": "Make haste, my beloved,"
    }
  ]
}