{
  "version": "WEBUS",
  "book": "PSA",
  "chapter": 115,
  "verses": [
    {
      "verse": "1",
      "text": "Not to us, Yahweh, not to us,"
    },
    {
      "verse": "2",
      "text": "Why should the nations say,"
    },
    {
      "verse": "3",
      "text": "But our God is in the heavens."
    },
    {
      "verse": "4",
      "text": "Their idols are silver and gold,"
    },
    {
      "verse": "5",
      "text": "They have mouths, but they don’t speak."
    },
    {
      "verse": "6",
      "text": "They have ears, but they don’t hear."
    },
    {
      "verse": "7",
      "text": "They have hands, but they don’t feel."
    },
    {
      "verse": "8",
      "text": "Those who make them will be like them;"
    },
    {
      "verse": "9",
      "text": "Israel, trust in Yahweh!"
    },
    {
      "verse": "10",
      "text": "House of Aaron, trust in Yahweh!"
    },
    {
      "verse": "11",
      "text": "You who fear Yahweh, trust in Yahweh!"
    },
    {
      "verse": "12",
      "text": "Yahweh remembers us. He will bless us."
    },
    {
      "verse": "13",
      "text": "He will bless those who fear Yahweh,"
    },
    {
      "verse": "14",
      "text": "May Yahweh increase you more and more,"
    },
    {
      "verse": "15",
      "text": "Blessed are you by Yahweh,"
    },
    {
      "verse": "16",
      "text": "The heavens are Yahweh’s heavens,"
    },
    {
      "verse": "17",
      "text": "The dead don’t praise Yah,"
    },
    {
      "verse": "18",
      "text": "but we will bless Yah,"
    }
  ]
}