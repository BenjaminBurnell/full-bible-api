{
  "version": "WEBUS",
  "book": "PSA",
  "chapter": 97,
  "verses": [
    {
      "verse": "1",
      "text": "Yahweh reigns!"
    },
    {
      "verse": "2",
      "text": "Clouds and darkness are around him."
    },
    {
      "verse": "3",
      "text": "A fire goes before him,"
    },
    {
      "verse": "4",
      "text": "His lightning lights up the world."
    },
    {
      "verse": "5",
      "text": "The mountains melt like wax at the presence of Yahweh,"
    },
    {
      "verse": "6",
      "text": "The heavens declare his righteousness."
    },
    {
      "verse": "7",
      "text": "Let all them be shamed who serve engraved images,"
    },
    {
      "verse": "#",
      "text": "Worship him, all you gods!"
    },
    {
      "verse": "8",
      "text": "Zion heard and was glad."
    },
    {
      "verse": "9",
      "text": "For you, Yahweh, are most high above all the earth."
    },
    {
      "verse": "10",
      "text": "You who love Yahweh, hate evil!"
    },
    {
      "verse": "11",
      "text": "Light is sown for the righteous,"
    },
    {
      "verse": "12",
      "text": "Be glad in Yahweh, you righteous people!"
    }
  ]
}