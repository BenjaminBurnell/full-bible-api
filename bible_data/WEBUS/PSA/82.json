{
  "version": "WEBUS",
  "book": "PSA",
  "chapter": 82,
  "verses": [
    {
      "verse": "1",
      "text": "God presides in the great assembly."
    },
    {
      "verse": "2",
      "text": "“How long will you judge unjustly,"
    },
    {
      "verse": "3",
      "text": "“Defend the weak, the poor, and the fatherless."
    },
    {
      "verse": "4",
      "text": "Rescue the weak and needy."
    },
    {
      "verse": "5",
      "text": "They don’t know, neither do they understand."
    },
    {
      "verse": "6",
      "text": "I said, “You are gods,"
    },
    {
      "verse": "7",
      "text": "Nevertheless you shall die like men,"
    },
    {
      "verse": "8",
      "text": "Arise, God, judge the earth,"
    }
  ]
}