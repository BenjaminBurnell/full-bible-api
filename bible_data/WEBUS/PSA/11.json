{
  "version": "WEBUS",
  "book": "PSA",
  "chapter": 11,
  "verses": [
    {
      "verse": "1",
      "text": "In Yahweh, I take refuge."
    },
    {
      "verse": "2",
      "text": "For, behold, the wicked bend their bows."
    },
    {
      "verse": "3",
      "text": "If the foundations are destroyed,"
    },
    {
      "verse": "4",
      "text": "Yahweh is in his holy temple."
    },
    {
      "verse": "5",
      "text": "Yahweh examines the righteous,"
    },
    {
      "verse": "6",
      "text": "On the wicked he will rain blazing coals;"
    },
    {
      "verse": "7",
      "text": "For Yahweh is righteous."
    }
  ]
}