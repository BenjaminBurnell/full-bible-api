{
  "version": "WEBUS",
  "book": "PSA",
  "chapter": 148,
  "verses": [
    {
      "verse": "1",
      "text": "Praise Yah!"
    },
    {
      "verse": "2",
      "text": "Praise him, all his angels!"
    },
    {
      "verse": "3",
      "text": "Praise him, sun and moon!"
    },
    {
      "verse": "4",
      "text": "Praise him, you heavens of heavens,"
    },
    {
      "verse": "5",
      "text": "Let them praise Yahweh’s name,"
    },
    {
      "verse": "6",
      "text": "He has also established them forever and ever."
    },
    {
      "verse": "7",
      "text": "Praise Yahweh from the earth,"
    },
    {
      "verse": "8",
      "text": "lightning and hail, snow and clouds,"
    },
    {
      "verse": "9",
      "text": "mountains and all hills,"
    },
    {
      "verse": "10",
      "text": "wild animals and all livestock,"
    },
    {
      "verse": "11",
      "text": "kings of the earth and all peoples,"
    },
    {
      "verse": "12",
      "text": "both young men and maidens,"
    },
    {
      "verse": "13",
      "text": "Let them praise Yahweh’s name,"
    },
    {
      "verse": "14",
      "text": "He has lifted up the horn of his people,"
    }
  ]
}