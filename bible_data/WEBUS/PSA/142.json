{
  "version": "WEBUS",
  "book": "PSA",
  "chapter": 142,
  "verses": [
    {
      "verse": "1",
      "text": "I cry with my voice to Yahweh."
    },
    {
      "verse": "2",
      "text": "I pour out my complaint before him."
    },
    {
      "verse": "3",
      "text": "When my spirit was overwhelmed within me,"
    },
    {
      "verse": "4",
      "text": "Look on my right, and see;"
    },
    {
      "verse": "5",
      "text": "I cried to you, Yahweh."
    },
    {
      "verse": "6",
      "text": "Listen to my cry,"
    },
    {
      "verse": "7",
      "text": "Bring my soul out of prison,"
    }
  ]
}