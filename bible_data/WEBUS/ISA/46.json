{
  "version": "WEBUS",
  "book": "ISA",
  "chapter": 46,
  "verses": [
    {
      "verse": "1",
      "text": "Bel bows down."
    },
    {
      "verse": "2",
      "text": "They stoop and they bow down together."
    },
    {
      "verse": "3",
      "text": "“Listen to me, house of Jacob,"
    },
    {
      "verse": "4",
      "text": "Even to old age I am he,"
    },
    {
      "verse": "5",
      "text": "“To whom will you compare me, and consider my equal,"
    },
    {
      "verse": "6",
      "text": "Some pour out gold from the bag,"
    },
    {
      "verse": "7",
      "text": "They bear it on their shoulder."
    },
    {
      "verse": "8",
      "text": "“Remember this, and show yourselves men."
    },
    {
      "verse": "9",
      "text": "Remember the former things of old;"
    },
    {
      "verse": "10",
      "text": "I declare the end from the beginning,"
    },
    {
      "verse": "11",
      "text": "I call a ravenous bird from the east,"
    },
    {
      "verse": "12",
      "text": "Listen to me, you stubborn-hearted,"
    },
    {
      "verse": "13",
      "text": "I bring my righteousness near."
    }
  ]
}